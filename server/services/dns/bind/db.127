;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	server.sectesting.com. root.sectesting.com. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	server.
13	IN	PTR	server.sectesting.com.
server	A	192.168.0.13
